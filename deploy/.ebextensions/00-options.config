option_settings:
  aws:elasticbeanstalk:environment:proxy:
    ProxyServer: nginx
  aws:elasticbeanstalk:application:environment:
    SERVER_PORT: 5000
    SPRING_PROFILES_ACTIVE: "prod"
  # La configuración de JVM debería ser así:
  aws:elasticbeanstalk:container:java:
    Xms: 256m
    Xmx: 512m
    # No uses JVMOptions como un parámetro directo